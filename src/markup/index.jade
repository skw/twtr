!!! 5
html
	head
		meta(charset='utf-8')
		meta(name='viewport', content='width=device-width, initial-scale=1.0, user-scalable=no')
		title Twitter Example
		link(rel='stylesheet', type='text/css', href='../css/style.css')
		//if lt IE
			script(src='http://html5shiv.googlecode.com/svn/trunk/html5.js')
	body
		// header
		.header.noise
			.container
				.row
					.span4
						header
							a.logo(href='/')
								| Twtr
					.span8
						a(href='http://twitter.com/shaunkirkwong')
							| @shaunkirkwong
						|  /  
						a(href='https://github.com/shaunwong')
							| github
		// main
		.main
			.container
				.row
					.span4.searchbar
						form.form-search
							input(type='text',placeholder='#').input-medium.search-query
							button(type='submit').btn
								| search
					.span8.results
						table.table.table-striped
							thead
								tr
									th #
									th id
									th tweet
							tbody
								tr
									td 1
									td @fat
									td I like cake
		// footer
		.footer.noise
			footer
				span
					a(href='//inke.ca')
						| Â© 2013 Shaun Kirk Wong.
		#scrolltop.noise
			| &uarr;
